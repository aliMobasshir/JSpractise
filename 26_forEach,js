// coding.forEach(printMe)

//apart from the direct element we can also access index and whole array:-

// coding.forEach( (item, index, arr)=> {
//     console.log(item, index, arr);
// } )

// important- we can use it for array of objects:-

const myCoding = [
    {
        languageName: "javascript",
        languageFileName: "js"
    },
    {
        languageName: "java",
        languageFileName: "java"
    },
    {
        languageName: "python",
        languageFileName: "py"
    },
]

myCoding.forEach( (item) => {     //when we pass parameter as item or anything else not elememts will be referred by that name
    
    console.log(item.languageName);
} )